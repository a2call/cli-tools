{
    "name": "c5dev/scaffolder",
    "description": "A package, theme & block scaffolder for concrete5.",
    "require": {
        "symfony/console": "~3.0",
        "illuminate/bus": "5.1.*",
        "illuminate/container": "5.1.*",
        "illuminate/support": "5.1.*",
        "illuminate/filesystem": "5.1.*",
        "illuminate/contracts": "5.1.*",
        "c5dev/package-boilerplate": "dev-master",
        "c5dev/theme-boilerplate": "dev-master",
        "c5dev/block-boilerplate": "dev-master",
        "symfony/process": "^3.1",
        "psy/psysh": "@stable"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Oliver Green",
            "email": "oliver@c5dev.com"
        }
    ],
    "repositories": [
    {
        "type": "vcs",
        "url": "https://github.com/olsgreen/installers.git"
    }
    ],
    "autoload": { "psr-4": {"C5Dev\\Scaffolder\\": "src/C5Dev/Scaffolder"}},
    "bin": ["bin/scaffolder"],
    "minimum-stability": "dev",
    "config": {
        "preferred-install": "dist"
    }
}
